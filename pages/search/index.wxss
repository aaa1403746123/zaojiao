page{
    padding: 20rpx;
}
.search_content{
    display:flex;
    height: 60rpx;
}
.search{
    border:0.5px solid grey;
    flex:1;
    height: 100%;
    padding-left: 20rpx;
    border-radius: 15px;
}
button{
    width: 80rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    border:0.5px solid grey;
    align-items: center;
    margin: 0 10rpx;
}
.search_item{
    border-bottom:1px solid black;
    padding: 20rpx;
}